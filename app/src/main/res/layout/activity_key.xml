    <TextView
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:id="@+id/key_M1"
        android:background="#ff0000"
        android:text="快捷拨号1"
        android:gravity="center"
        android:layout_marginTop="25dp"
        android:layout_marginStart="25dp"/>

    <TextView
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:id="@+id/key_M2"
        android:background="#ff0000"
        android:text="快捷拨号2"
        android:gravity="center"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/key_M1"/>

    <TextView
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:id="@+id/key_M3"
        android:background="#ff0000"
        android:text="快捷拨号3"
        android:gravity="center"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/key_M2"/>

    <TextView
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:id="@+id/key_M4"
        android:background="#ff0000"
        android:text="删除返回键"
        android:gravity="center"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/key_M3"/>

    <TextView
        android:id="@+id/key_1"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_M1"
        android:layout_below="@+id/key_M1"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="1" />

    <TextView
        android:id="@+id/key_2"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_M1"
        android:layout_toEndOf="@+id/key_M1"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="2" />

    <TextView
        android:id="@+id/key_3"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_M1"
        android:layout_toEndOf="@+id/key_M2"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="3" />

    <TextView
        android:id="@+id/key_4"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_M1"
        android:layout_below="@+id/key_1"
        android:layout_centerVertical="true"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="4" />

    <TextView
        android:id="@+id/key_5"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/key_M1"
        android:layout_below="@+id/key_2"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="5" />

    <TextView
        android:id="@+id/key_6"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/key_M2"
        android:layout_below="@+id/key_3"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="6" />

    <TextView
        android:id="@+id/key_7"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_M1"
        android:layout_below="@+id/key_4"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="7" />

    <TextView
        android:id="@+id/key_8"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_4"
        android:layout_toEndOf="@+id/key_M1"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="8" />

    <TextView
        android:id="@+id/key_9"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_4"
        android:layout_toEndOf="@+id/key_M2"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="9" />

    <TextView
        android:id="@+id/key_star"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_M1"
        android:layout_below="@+id/key_7"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="stat" />

    <TextView
        android:id="@+id/key_0"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_7"
        android:layout_toEndOf="@+id/key_M1"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="0" />

    <TextView
        android:id="@+id/key_pound"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_7"
        android:layout_toEndOf="@+id/key_M2"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="pound" />

    <TextView
        android:id="@+id/key_voldown"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_M1"
        android:layout_below="@+id/key_star"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="vol-" />

    <TextView
        android:id="@+id/key_volup"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_star"
        android:layout_toEndOf="@+id/key_M1"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="vol+" />

    <TextView
        android:id="@+id/key_redial"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_star"
        android:layout_toEndOf="@+id/key_M2"
        android:layout_marginLeft="37dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="Redial" />

    <TextView
        android:id="@+id/key_wifiap"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/key_M1"
        android:layout_marginEnd="99dp"
        android:background="#ff0000"
        android:gravity="center"
        android:text="wifi热点" />

    <TextView
        android:id="@+id/key_up"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_wifiap"
        android:layout_below="@+id/key_M1"
        android:background="#ff0000"
        android:gravity="center"
        android:text="up" />

    <TextView
        android:id="@+id/key_ok"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_wifiap"
        android:layout_below="@+id/key_1"
        android:background="#ff0000"
        android:gravity="center"
        android:text="ok" />

    <TextView
        android:id="@+id/key_left"
        android:layout_width="40dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="100dp"
        android:layout_toStartOf="@+id/key_wifiap"
        android:background="#ff0000"
        android:gravity="center"
        android:text="left" />

    <TextView
        android:id="@+id/key_right"
        android:layout_width="40dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-100dp"
        android:layout_marginTop="100dp"
        android:layout_toEndOf="@+id/key_wifiap"
        android:background="#ff0000"
        android:gravity="center"
        android:text="right" />

    <TextView
        android:id="@+id/key_down"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_wifiap"
        android:layout_below="@+id/key_4"
        android:background="#ff0000"
        android:gravity="center"
        android:text="down" />

    <TextView
        android:id="@+id/key_power"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/key_7"
        android:layout_marginEnd="138dp"
        android:background="#ff888888"
        android:gravity="center"
        android:text="power" />

    <TextView
        android:id="@+id/key_handfree"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_below="@+id/key_7"
        android:layout_marginStart="-138dp"
        android:layout_toEndOf="@+id/key_power"
        android:background="#ff0000"
        android:gravity="center"
        android:text="handfree" />

    <TextView
        android:id="@+id/key_dial"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/key_power"
        android:layout_below="@+id/key_star"
        android:background="#ff0000"
        android:gravity="center"
        android:text="dial" />

</RelativeLayout>